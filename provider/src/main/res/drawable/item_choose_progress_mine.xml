<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/dp_3" />
            <solid android:color="@color/color_f5f5f5" />
            <size
                android:width="1dp"
                android:height="1dp" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dp_3" />
                <gradient
                    android:angle="0"
                    android:endColor="@color/color_cb2b2b"
                    android:startColor="@color/common_color_e25e49" />
                <size
                    android:width="1dp"
                    android:height="1dp" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dp_3" />
                <gradient
                    android:angle="0"
                    android:endColor="@color/color_cb2b2b"
                    android:startColor="@color/common_color_e25e49" />
                <size
                    android:width="1dp"
                    android:height="1dp" />
            </shape>
        </clip>
    </item>

</layer-list>