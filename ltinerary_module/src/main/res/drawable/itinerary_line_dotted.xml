<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="20dp"
        android:right="20dp">
        <shape android:shape="line">
            <stroke
                android:width="@dimen/dp_1"
                android:color="@color/color_D4D4D4"
                android:dashWidth="@dimen/dp_5"
                android:dashGap="@dimen/dp_5" />
            <size android:width="20dp" />
        </shape>
    </item>
    <item android:left="200dp">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/color_D4D4D4" />
            <size
                android:width="20dp"
                android:height="20dp" />
        </shape>
    </item>
    <item android:right="200dp">
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/color_D4D4D4" />
            <size
                android:width="20dp"
                android:height="20dp" />
        </shape>
    </item>
</layer-list>